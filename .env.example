# Shopify App Configuration Template
# Copy this file to .env.staging or .env.production and fill in your values

# Shopify App Credentials
SHOPIFY_API_KEY=your_api_key_here
SHOPIFY_API_SECRET=your_api_secret_here

# Database
DATABASE_URL=postgresql://localhost:5432/magicsell_dev

# App URL (will be different per environment)
SHOPIFY_APP_URL=https://your-app-url.com

# Theme Extension UID (get this after deploying your theme extension)
# This UID is app-specific and different for staging vs production
THEME_EXTENSION_UID=your_extension_uid_here

# Node Environment
NODE_ENV=development
